{
  "name": "Laptop Info Extension",
  "version": "1.0",
  "manifest_version": 2,

  "description": "Easily readable computers Info",

  "icons": {
    "128": "siberian-husky.png"
  },

  "browser_action":
   {
     "default_popup": "popup.html"
   },
   "content_scripts": [
    {
    "matches": ["https://*.bestbuy.com/*"],
    "js": ["content_script.js"],
    "run_at": "document_end"
    }
  ],
  "permision": [
    "http://*/*", "https://*/*"
  ]
}
